{
    "name": "supertools",
    "version": "1.5.0",
    "description": "SuperData SQL generation tools",
    "main": "index.js",
    "scripts": {
        "start": "electron ./ dev",
        "production": "electron ./ production",
        "package": "electron-packager ./ SuperTools --platform=win32 --arch=x64 --overwrite --prune=true --out=release-builds --icon=icon/icon.ico",
        "packagelinux": "electron-packager ./ SuperTools --platform=linux --arch=x64 --overwrite --prune=true --out=release-builds --icon=icon/icon.ico",
        "packagemac": "electron-packager ./ SuperTools --platform=darwin --arch=x64 --overwrite --prune=true --out=release-builds --icon=icon/icon.icns"
    },
    "author": "David.Wei@schleuniger.com",
    "license": "Schleuniger Software License",
    "dependencies": {
        "co": "^4.6.0",
        "co-mssql": "^1.3.0",
        "iconv-lite": "^0.4.24",
        "js-base64": "^2.4.9",
        "moment": "^2.22.2",
        "mssql": "^4.2.1",
        "ping": "^0.2.2",
        "sqlite-sync": "^0.3.8",
        "underscore": "^1.9.1"
    },
    "devDependencies": {
        "electron": "^4.0.0",
        "electron-packager": "^12.2.0"
    }
}
